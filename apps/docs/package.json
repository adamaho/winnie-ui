{
  "name": "docs",
  "type": "module",
  "version": "0.0.16",
  "private": true,
  "scripts": {
    "astro": "astro",
    "build": "rimraf dist .astro && astro check && astro build",
    "clean": "rimraf node_modules dist .astro",
    "dev": "astro dev",
    "dev:sst": "sst dev astro dev",
    "deploy:production": "sst deploy --stage=production --print-logs",
    "deploy:staging": "sst deploy --stage=staging",
    "lint:check": "biome lint ./src --no-errors-on-unmatched",
    "lint:write": "biome lint ./src --write --unsafe --no-errors-on-unmatched",
    "format:check": "biome format ./src --no-errors-on-unmatched",
    "format:write": "biome format ./src --write --no-errors-on-unmatched",
    "remove:production": "sst remove --stage=staging",
    "remove:staging": "sst remove --stage=staging",
    "preview": "astro preview",
    "start": "astro dev"
  },
  "dependencies": {
    "@astrojs/check": "0.9.3",
    "@astrojs/mdx": "3.1.6",
    "@astrojs/react": "3.6.2",
    "@fontsource/geist-mono": "5.1.0",
    "@fontsource/inter": "5.1.0",
    "@radix-ui/react-collapsible": "1.1.0",
    "@radix-ui/react-dialog": "1.1.1",
    "@radix-ui/react-scroll-area": "1.1.0",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "@winnie-ui/css": "workspace:*",
    "astro": "4.15.6",
    "astro-expressive-code": "0.36.1",
    "astro-sst": "2.43.5",
    "clsx": "2.1.1",
    "cmdk": "1.0.0",
    "flexsearch": "0.7.43",
    "lucide-react": "0.441.0",
    "react": "^18.3.1",
    "react-aria-components": "1.3.3",
    "react-dom": "18.3.1",
    "recharts": "2.12.7",
    "sst": "3.1.10",
    "typescript": "5.6.2"
  },
  "devDependencies": {
    "@types/flexsearch": "0.7.6",
    "rimraf": "6.0.1"
  }
}
