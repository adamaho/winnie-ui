{
	"name": "winnie-ui",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"build": "turbo run build --cache-dir=.turbo",
		"biome:check": "biome check --write --unsafe --no-errors-on-unmatched",
		"changeset": "changeset",
		"changeset:publish": "changeset publish",
		"clean": "turbo run clean && rimraf node_modules",
		"tsc": "turbo run tsc",
		"prepare": "husky"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@changesets/changelog-github": "0.5.0",
		"@changesets/cli": "2.27.7",
		"husky": "9.1.5",
		"lint-staged": "15.2.9",
		"rimraf": "6.0.1",
		"turbo": "2.1.0"
	},
	"packageManager": "pnpm@9.7.1"
}
