{
  "name": "winnie-ui",
  "version": "0.0.1",
  "description": "A collection of tools for writing React applications",
  "author": "Adam Aho",
  "license": "ISC",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean && rimraf node_modules",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,yml,yaml,.md,.mdx}\"",
    "lint": "turbo run lint",
    "stylelint": "turbo run stylelint",
    "tsc": "turbo run tsc",
    "prepare": "husky"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.7",
    "prettier": "3.3.2",
    "prettier-plugin-astro": "0.14.0",
    "prettier-plugin-tailwindcss": "0.6.5",
    "rimraf": "5.0.7",
    "turbo": "2.0.6"
  },
  "engines": {
    "node": ">=21.7.3",
    "pnpm": ">=9.3.0"
  },
  "packageManager": "pnpm@9.3.0"
}
