{
  "name": "winnie-ui",
  "version": "0.0.1",
  "description": "A collection of tools for writing React applications",
  "author": "Adam Aho",
  "license": "ISC",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "changeset": "changeset",
    "clean": "turbo run clean && rimraf node_modules",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,yml,yaml,.md,.mdx}\"",
    "lint": "turbo run lint",
    "publish": "turbo build --cache-dir=.turbo && changeset publish",
    "stylelint": "turbo run stylelint",
    "tsc": "turbo run tsc",
    "prepare": "husky"
  },
  "devDependencies": {
    "@changesets/cli": "2.27.7",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "husky": "9.1.4",
    "lint-staged": "15.2.8",
    "prettier": "3.3.3",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.6.5",
    "rimraf": "6.0.1",
    "turbo": "2.0.12"
  },
  "engines": {
    "node": ">=22.5.0",
    "pnpm": ">=9.7.0"
  },
  "packageManager": "pnpm@9.7.0"
}
